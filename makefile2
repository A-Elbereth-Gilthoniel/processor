TARGET = asmblr

asmblr : assembler.o file.o coder.o string.o checking2.o
	gcc assembler.o file.o coder.o string.o checking2.o -o asmblr

assembler.o : assembler.c
	gcc -c assembler.c -o assembler.o

file.o : file.c
	gcc -c file.c -o file.o

coder.o : coder.c
	gcc -c coder.c -o coder.o

string.o : string.c
	gcc -c string.c -o string.o

checking2.o : checking2.c
	gcc -c checking2.c -o checking2.o

.PHONY : clean
clean :
	del *.o $(TARGET).exe -rf
